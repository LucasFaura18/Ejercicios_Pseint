Proceso sin_titulo
	Definir contador Como Entero;
	Para contador<- 1 hasta 10 con paso 1 Hacer
		Escribir contador;
	FinPara
	contador<-1;
	Mientras contador<=10 Hacer
		Escribir contador;
		contador<-contador+1;
	FinMientras
	contador<-1;
	Repetir 
		Escribir contador;
		contador<-contador+1;
	Hasta Que contador=11

FinProceso
